OUTPUT_FILE=$1
PROMPT="Postprocessing ["
PARAMS="-code ./post-process.js -out-file ./processed-json/$1.json"
shift;

while [ $# -ne 0 ] ; do
  PARAMS="$PARAMS -file ./scraped-json/$1.json"
  PROMPT="$PROMPT $1"
  shift;
done
PROMPT="$PROMPT ] to $OUTPUT_FILE"

echo $PROMPT
java -jar ../../../tools/juggler.jar $PARAMS
